<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern> [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="trackModel" level="INFO"/>
    <logger name="CTCOffice" level="INFO"/>
    <logger name="trainModel" level="ERROR"/>
    <logger name="trainController" level="ERROR"/>
    <logger name="waysideController" level="INFO"/>

    <root level="WARN">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>