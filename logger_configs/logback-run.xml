<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern> [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

<!--    <logger name="Trains.trackModel" level="DEBUG"/>-->
        <logger name="CTCOffice" level="WARN"/>
<!--    <logger name="Trains.trainModel" level="INFO"/>-->
<!--    <logger name="Trains.trainController" level="INFO"/>-->
        <logger name="waysideController" level="WARN"/>

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>